 "html": "<h3>Daily Attendance Report</h3>\n\n<table class=\"table table-bordered\">\n    <thead>\n        <tr>\n            <th>Employee</th>\n            <th>Date</th>\n            <th>In Time</th>\n            <th>AM/PM</th>\n            <th>Signature</th>\n            <th>Out Time</th>\n            <th>AM/PM</th>\n            <th>Signature</th>\n        </tr>\n    </thead>\n    <tbody>\n        {%- for row in doc.daily_attendance_child -%}\n        <tr>\n            <td>{{ row.employee }}</td>\n            <td>{{ row.date }}</td>\n            <td>{{ row.in_time }}</td>\n            <td>{{ row.am_pm_in }}</td>\n            <td>{{ row.signature_in }}</td>\n            <td>{{ row.out_time }}</td>\n            <td>{{ row.am_pm_out }}</td>\n            <td>{{ row.signature_out }}</td>\n        </tr>\n        {%- endfor -%}\n    </tbody>\n</table>\n",
